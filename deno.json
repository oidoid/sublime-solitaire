{
  "compilerOptions": {
    "exactOptionalPropertyTypes": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "useUnknownInCatchVariables": true,
    "lib": ["dom", "deno.ns"]
  },
  "fmt": {
    "files": { "exclude": ["dist"] },
    "options": { "semiColons": false, "singleQuote": true }
  },
  "imports": {
    "@/atlas-pack": "https://deno.land/x/atlas_pack@v7.1.0/mod.ts",
    "@/mem": "https://deno.land/x/mem@v9.0.0/mod.ts",
    "@/ooz": "https://deno.land/x/ooz@v0.0.16/mod.ts",
    "@/solitaire": "https://deno.land/x/solitaire@v0.0.7/mod.ts",
    "@/super-patience": "./mod.ts",
    "@/void": "https://deno.land/x/oid@v0.0.9/mod.ts",
    "std/": "https://deno.land/std@0.179.0/"
  },
  "lint": {
    "files": { "exclude": ["dist"] },
    "rules": {
      "exclude": [
        "no-empty-interface",
        "no-inferrable-types",
        "no-misused-new",
        "no-namespace"
      ]
    }
  },
  "lock": false,
  "name": "super-patience",
  "test": { "files": { "exclude": ["dist"] } },
  "version": "1.1.2"
}
